class Solution{
  public:
    // Function to find floor of x
    // n: size of vector
    // x: element whose floor is to find
    int findFloor(vector<long long> v, long long n, long long x){
        
        long long  l=0, r=n-1;
        
        
        
        long long ans=n;
        
        while(l<=r){
            
            long long mid=(l+r)/2;
            if(x==v[mid]) return mid;
            else if(x<v[mid]) r=mid-1;
            else l=mid+1, ans=mid;
        }
        
        if(x<v[0]) return -1;
        else return ans;
        
        
    }
};
